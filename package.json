{
  "name": "chat-multiregion-site",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm:dev-client\" \"npm:dev-server\"",
    "dev-client": "cd client && npm run dev",
    "dev-server": "cd server && nodemon server.js",
    "build": "cd client && npm run build",
    "start": "node server/server.js"
  },
  "dependencies": {
    "concurrently": "^8.2.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.22"
  }
}